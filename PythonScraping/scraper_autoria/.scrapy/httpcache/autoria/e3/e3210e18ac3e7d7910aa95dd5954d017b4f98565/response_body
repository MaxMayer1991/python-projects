             window._gaq = {};
            window._gaq.push =  function(array){
                if(typeof window.ga !== 'undefined' ) {
                    if (array[0] === '_trackPageview') {
                        window.ga('send', 'pageview');
                    } else if (array[0] === '_trackEvent') {
                        window.ga('send','event', array[1], array[2], array[3]);
                    }
                }
            }

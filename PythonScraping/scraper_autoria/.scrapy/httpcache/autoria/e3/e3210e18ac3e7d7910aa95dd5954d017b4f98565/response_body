                if (Number(urlParams.get('siteAppBanner')) === 1) {
                  isSiteAppBanner = true;
                }
              }
              if(isUserAgentAppNameAuto || isSiteAppBanner) {
                document.querySelector('.mobile-header-wv').style.display = 'none';
                document.body.classList.add('web-view');
                __WEB_VIEW__ = true;
              }
            } catch (e) {
              console.log(e);

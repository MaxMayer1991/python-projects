      var validRMetrics = ['r_audience', 'r_source', 'r_medium', 'r_campaign', 'r_options'];
      var validBotSource = { tl: 'telegram', vb: 'viber', fb: 'facebook' };

      try {
          var rSearchString = window.location.search || window.location.hash.split("?")[1] || '';
